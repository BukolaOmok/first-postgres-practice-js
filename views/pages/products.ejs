<%- include("../partials/header.ejs", {title: "Products and suppliers"}) %>

<div class="products">
  <% for (let product of products){ %>
  <div class="product">
    <div><%= product.product_name %></div>
    <div><%= product.units_in_stock %></div>
    <div><%= product.units_on_order %></div>
    <div><%= product.reorder_level %></div>
    <div><%= product.quantity_per_unit %></div>
    <div><%= product.company_name %></div>
  </div>
  <% } %>
</div>
</body>

</html>